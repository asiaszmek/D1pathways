        <Product  specieID="pPDE10"                 />
        <Product  specieID="AMP"                    />

        <forwardRate>  8.4e-3        </forwardRate>
        <reverseRate>  0.0          </reverseRate>
        <Q10>          0.2          </Q10>
    </Reaction>

    <!--3. AMP             -> ATP              (in xpp: katp=10) -->
    <Reaction name = "AMP--ATP" id="AMP--ATP_id">
        <Reactant specieID="AMP"                    />
