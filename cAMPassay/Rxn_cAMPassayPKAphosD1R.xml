        <forwardRate> 1.00e-03      </forwardRate>
        <reverseRate> 0.0           </reverseRate>
        <Q10>         0.2           </Q10>
    </Reaction>

<!-- ******* PKA from Asia Hippo Model ********* -->     
    <!--1) PKA + 2cAMP <-> PKAcAMP2 -->
    <Reaction name = "PKA_bind" id="PKA_bind">
